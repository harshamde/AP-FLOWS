                  {
                    "id": "Andhrapradesh",
                    "title": "Andhrapradesh"
                  }
                ],
                "required": true,
                "type": "Dropdown"
              },
              {
                "label": "జిల్లా",
                "name": "district",
                "data-source": "${data.districtList}",
                "required": true,
                "enabled": true,
                "on-select-action": {
                  "name": "data_exchange",
                  "payload": {
                    "name": "${form.name}",
                    "surname": "${form.surname}",
                    "gender": "${form.gender}",
                    "age": "${form.age}",
                    "occupation": "${form.occupation}",
                    "addressLine1": "${form.addressLine1}",
                    "addressLine2": "${form.addressLine2}",
                    "state": "${form.state}",
                    "district": "${form.district}",
                    "mandal": "${form.mandal}",
                    "village": "${form.village}",
                    "city": "${form.city}",
                    "pinCode": "${form.pinCode}",
                    "mobileNumber": "${form.mobileNumber}",
                    "phoneNo": "${form.phoneNo}",
                    "emailId": "${form.emailId}",
                    "extraDetails": "${data.extraDetails}",
                    "onSelectAction": "on-district-click"
                  }
                },
                "type": "Dropdown"
              },
              {
                "label": "మండలం",
                "name": "mandal",
                "data-source": "${data.mandalList}",
                "required": true,
                "visible": "${data.isMandalVisible}",
                "on-select-action": {
                  "name": "data_exchange",
