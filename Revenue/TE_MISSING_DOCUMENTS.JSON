      "id": "WELCOME",
      "title": "ప్రాథమిక వివరాలు",
      "layout": {
        "type": "SingleColumnLayout",
        "children": [
          {
            "type": "Form",
            "name": "form",
            "children": [
              {
                "text": "తప్పిపోయిన లేదా కోల్పోయిన పత్రాల అప్లికేషన్",
                "visible": true,
                "type": "TextHeading"
              },
              {
                "label": "పేరు",
                "name": "name",
                "required": true,
                "min-chars": 4,
                "max-chars": 49,
                "type": "TextInput"
              },
              {
                "label": "ఇంటిపేరు",
                "name": "surname",
                "required": true,
                "type": "TextInput"
              },
              {
                "label": "లింగం",
                "name": "gender",
                "data-source": "${data.genderList}",
                "required": true,
                "type": "Dropdown"
              },
              {
                "label": "వయస్సు",
                "name": "age",
                "required": true,
                "input-type": "number",
                "min-chars": 1,
                "max-chars": 2,
                "type": "TextInput"
              },
              {
                "label": "వృత్తి",
                "name": "occupation",
                "required": true,
                "min-chars": 4,
                "max-chars": 29,
                "type": "TextInput"
              },
              {
                "label": "చిరునామా లైన్ 1",
                "name": "addressLine1",
                "required": true,
                "max-length": 99,
                "type": "TextArea"
              },
              {
                "label": "చిరునామా లైన్ 2",
                "name": "addressLine2",
                "required": true,
                "max-length": 99,
                "type": "TextArea"
              },
              {
                "label": "రాష్ట్రం/UT",
                "name": "state",
