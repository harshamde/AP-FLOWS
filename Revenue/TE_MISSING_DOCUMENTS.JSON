              {
                "text": "${data.errorBody}",
                "visible": "${data.errorBodyVisible}",
                "font-weight": "italic",
                "strikethrough": false,
                "type": "TextBody"
              },
              {
                "label": "పోలీస్ స్టేషన్",
                "name": "policeStation",
                "required": true,
                "visible": "${data.isPoliceStationVisible}",
                "data-source": "${data.policeStationList}",
                "type": "Dropdown"
              },
              {
                "label": "సమీప ల్యాండ్‌మార్క్/ ఏరియా వివరణ",
                "name": "nearestLandmarkArea",
                "required": true,
                "visible": "${data.nearestLandmarkAreaVisible}",
                "type": "TextArea"
              },
              {
                "label": "పత్రం/కథనం మిస్ కావడానికి కారణం",
                "name": "reasonForMissingDocument",
                "required": true,
                "max-length": 200,
                "type": "TextArea"
              },
              {
                "label": "ID ప్రూఫ్ అప్‌లోడ్ చేయాలి",
                "name": "idProofToBeUploaded",
                "data-source": "${data.idProofToBeUploadedList}",
                "required": true,
                "type": "Dropdown"
              },
              {
                "label": "అడ్రస్ ప్రూఫ్ అప్‌లోడ్ చేయాలి",
                "name": "addressProofToBeUploaded",
                "data-source": "${data.addressProofToBeUploaded}",
                "required": true,
                "type": "Dropdown"
              },
              {
                "label": "డెలివరీ రకం",
                "name": "deliveryType",
                "data-source": [
                  {
                    "id": "మాన్యువల్",
                    "title": "మాన్యువల్"
                  }
                ],
                "required": true,
                "type": "Dropdown"
              },
              {
                "on-click-action": {
                  "name": "complete",
                  "payload": {
                    "informantName": "${data.informantName}",
                    "informantSurname": "${data.informantSurname}",
                    "relationshipWithApplicant": "${data.relationshipWithApplicant}",
                    "name": "${data.name}",
                    "surname": "${data.surname}",
                    "gender": "${data.gender}",
                    "age": "${data.age}",
                    "occupation": "${data.occupation}",
                    "addressLine1": "${data.addressLine1}",
                    "addressLine2": "${data.addressLine2}",
                    "state": "${data.state}",
                    "district": "${data.district}",
                    "mandal": "${data.mandal}",
                    "village": "${data.village}",
                    "city": "${data.city}",
                    "pinCode": "${data.pinCode}",
                    "mobileNumber": "${data.mobileNumber}",
                    "phoneNo": "${data.phoneNo}",
                    "emailId": "${data.emailId}",
                    "extraDetails": "${data.extraDetails}",
                    "missingDocument": "${form.missingDocument}",
                    "districtCommissionerate": "${form.districtCommissionerate}",
                    "policeStationJurisdictionKnown": "${form.policeStationJurisdictionKnown}",
                    "policeStation": "${form.policeStation}",
                    "nearestLandmarkArea": "${form.nearestLandmarkArea}",
                    "reasonForMissingDocument": "${form.reasonForMissingDocument}",
                    "idProofToBeUploaded": "${form.idProofToBeUploaded}",
                    "addressProofToBeUploaded": "${form.addressProofToBeUploaded}",
                    "deliveryType": "${form.deliveryType}"
                  }
                },
                "label": "సమర్పించండి",
                "type": "Footer"
              }
            ],
            "init-values": {
              "name": "${data.nameInitValue}",
              "surname": "${data.surnameInitValue}",
              "state": "Andhrapradesh",
              "pinCode": "${data.pinCodeInitValue}",
              "mobileNumber": "${data.mobileNoInitValue}",
              "phoneNo": ""
            }
          }
        ]
      },
      "terminal": true
    }
  ]
}